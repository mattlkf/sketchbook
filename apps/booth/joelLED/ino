void setup(){
  pinMode(3,OUTPUT);
  pinMode(5,OUTPUT);
  pinMode(9,OUTPUT);
  pinMode(10,OUTPUT);
  pinMode(11,OUTPUT);
}

void loop(){

  analogWrite(3, 127.0 + 127.0 * sin((float)millis() / 50.0));
  analogWrite(5, 127.0 + 127.0 * sin((float)millis() / 300.0));
  analogWrite(9, 127.0 + 127.0 * sin((float)millis() / 100.0));
  analogWrite(10, 127.0 + 127.0 * sin((float)millis() / 200.0));
  analogWrite(11, 127.0 + 127.0 * sin((float)millis() / 300.0));


}